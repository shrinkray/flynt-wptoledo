<flynt-component name="NavigationFooterColumns" class="componentSpacing" data-theme="dark">
  <div class="container" data-flow="layout">
    {% if preContentHtml %}
      <div data-size="medium" data-align="left">
        {{ preContentHtml|e('wp_kses_post') }}
      </div>
    {% endif %}
    <nav class="navigation" aria-label="{{ labels.navigationAriaLabel|e }}">
      {% include "Partials/_menu.twig" with {items: menu.items, socialPlatformsTitle: socialPlatformsTitle, socialPlatforms: socialPlatforms, labels: labels} %}
    </nav>
  </div>
</flynt-component>
